    uart1_data_pins: uart_data@1 {
        groups = "uart1_data";
        function = "uart";
    };

    uart1_ctl_pins: uart_ctl@1 {
        groups = "uart1_ctl";
        function = "uart";
    };

    csi2_pins: csi@2 {
        groups = "csi2rw";
        function = "csi";
    };

    csi3_pins: csi@3 {
        groups = "csi3rw";
        function = "csi";
    };

    csi4_pins: csi@4 {
        groups = "csi4rw";
        function = "csi";
    };

    csi5_pins: csi@5 {
        groups = "csi5rw";
        function = "csi";
    };

    i2c0_pins: i2c@0 {
        groups = "i2c0";
        function = "i2c";
    };

    i2c1_pins: i2c@1 {
        groups = "i2c1";
        function = "i2c";
    };
};

&csi2{
    status = "okay";
    slavedev2: spi@0 {
        compatible = "rzv2m,slavedev";
        reg = <0>;
        spi-max-frequency = <25000000>;
        spi-cpha;
        spi-cpol;
    };
};

