/dts-v1/;
#include "r9a07g044l2.dtsi"
#include "rzg2-smarc.dtsi"

/ {
	model = "Evaluation board based on r9a07g044l2";
	compatible = "renesas,r9a07g044l2-dev", "renesas,r9a07g044l2";

	memory@47E00000 {
		device_type = "memory";
		/* first 126MB is reserved for secure area. */
		reg = <0x0 0x47E00000 0x0 0xF8200000>;
	};
};

