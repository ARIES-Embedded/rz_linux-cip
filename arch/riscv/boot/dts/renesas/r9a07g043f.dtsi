				"ch8", "ch9", "ch10", "ch11",
				"ch12", "ch13", "ch14", "ch15";
			dma-channels = <16>;
			#dma-cells = <1>;
			clocks = <&cpg CPG_MOD R9A07G043F_CLK_DMAC>;
