			reg = <0 0x10058800 0 0x44>;
			interrupts = <366  IRQ_TYPE_LEVEL_HIGH>,
				     <364 IRQ_TYPE_EDGE_RISING>,
				     <365 IRQ_TYPE_EDGE_RISING>,
				     <368 IRQ_TYPE_LEVEL_HIGH>,
				     <369 IRQ_TYPE_LEVEL_HIGH>,
