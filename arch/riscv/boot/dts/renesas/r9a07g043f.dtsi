					<459 IRQ_TYPE_LEVEL_HIGH>;

			clocks = <&cpg CPG_MOD R9A07G043F_CLK_CANFD>,
				 <&cpg CPG_CORE R9A07G043F_CLK_P0>;
			clock-names = "fck","canfd";
			resets = <&cpg R9A07G043F_CLK_CANFD>;
			power-dmains = <&cpg>;
			status = "disabled";
			channel0 {
				renesas,channel = <0>;
				status = "disabled";
			};
			channel1 {
				renesas,channel = <1>;
				status = "disabled";
			};
		};

