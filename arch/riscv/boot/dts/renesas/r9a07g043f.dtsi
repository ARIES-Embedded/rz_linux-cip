				<139 IRQ_TYPE_EDGE_RISING>,
				<140 IRQ_TYPE_EDGE_RISING>,
				<141 IRQ_TYPE_EDGE_RISING>;
			interrupt-names = "ch0", "ch1", "ch2", "ch3",
				"ch4", "ch5", "ch6", "ch7",
				"ch8", "ch9", "ch10", "ch11",
				"ch12", "ch13", "ch14", "ch15",
