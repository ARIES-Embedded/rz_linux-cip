			interrupts = < 138 IRQ_TYPE_LEVEL_HIGH>,
				     < 139 IRQ_TYPE_LEVEL_HIGH>;
			clocks = <&cpg CPG_MOD R9A07G043F_CLK_SDHI1>;
			resets = <&cpg R9A07G043F_CLK_SDHI1>;
