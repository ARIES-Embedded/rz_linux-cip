				<133 IRQ_TYPE_EDGE_RISING>,
				<134 IRQ_TYPE_EDGE_RISING>,
				<135 IRQ_TYPE_EDGE_RISING>,
				<136 IRQ_TYPE_EDGE_RISING>,
				<137 IRQ_TYPE_EDGE_RISING>,
