			clocks = <&cpg CPG_MOD R9A07G043F_CLK_TSU_PCLK>;
			resets = <&cpg R9A07G043F_CLK_TSU_PCLK>;
			clock-names = "tsu";
			thermal-cal = <1>;
			#thermal-sensor-cells = <0>;
			power-domains = <&cpg>;
		};

		dma0: dma-controller@11800000 {
			compatible = "renesas,r9a07g043f-dma",
				"renesas,rz-dmac";
			reg =   <0 0x11820000 0 0x10000>,
				<0 0x11830000 0 0x10000>;

			interrupts =
				<173 IRQ_TYPE_EDGE_RISING>,    
				<157 IRQ_TYPE_EDGE_RISING>,
				<158 IRQ_TYPE_EDGE_RISING>,
				<159 IRQ_TYPE_EDGE_RISING>,
				<160 IRQ_TYPE_EDGE_RISING>,
